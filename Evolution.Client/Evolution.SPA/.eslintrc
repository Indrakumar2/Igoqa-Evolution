{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "extends": "react-app",
  "rules": {
    "react/prop-types": 0,
    "react/no-typos":0,
    "no-unused-expressions":0,   
    "no-undef": [
      1
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-var": "error",
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ],
    "template-curly-spacing": [
      2,
      "always"
    ],
    "array-bracket-spacing": [
      2,
      "always"
    ],
    "object-curly-spacing": [
      2,
      "always"
    ],
    "prefer-const": "error",
    "react/no-string-refs": "warn",
    "react/no-deprecated": "off",
    "max-len": [
      "warn",
      {
        "code": 160
      }
    ]
   
  }
}
